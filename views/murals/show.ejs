<%- include ../partials/header.ejs %>

    <h1><%= mural.name %></h1>
    <ul>
      <li>Description: <%= mural.description %></li>
      <li>Artist: <%= artist.name %></li>
      <li>Neighborhood: <%= neighborhood.name %></li>
    </ul>

    <% let curPhoto = 1; %>
    <% while (curPhoto <= mural.photo_count) {%>
      <img src="http://res.cloudinary.com/murality/<%=mural.name+'/'+curPhoto%>.jpg" alt="mural" width="400" height="300">
      <% curPhoto++; %>
    <% } %>

<%- include ../partials/footer.ejs %>
